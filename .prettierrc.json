{
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "useTabs": false,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "overrides": [
        {
            "files": "*.scss",
            "options": {
                "singleQuote": false,
                "proseWrap": "never"
            }
        }
    ],
    "extends": [
        "airbnb",
        "plugin:prettier/recommended"
    ]
}